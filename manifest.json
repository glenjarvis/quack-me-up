{
  "manifest_version": 3,
  "name": "QuackMeUp",
  "version": "1.0",
  "description": "The best ducks in the world",
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["duckPicture.jpg", "quack.mp3"],
      "matches": ["<all_urls>"]
    }
  ]
}
